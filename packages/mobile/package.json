{
  "name": "mobile",
  "version": "0.10.12",
  "private": true,
  "workspaces": {
    "nohoist": [
      "mobx-react-lite"
    ]
  },
  "scripts": {
    "clean": "sh ./scripts/clean.sh",
    "android": "yarn build:provider && yarn build-and-run-android",
    "build-and-run-android": "npx react-native run-android --main-activity com.chainapsis.keplr.MainActivity",
    "ios": "yarn build:provider && npx react-native run-ios",
    "android:connect": "adb reverse tcp:8081 tcp:8081",
    "typecheck": "tsc",
    "build:provider": "tsc --project tsconfig.provider.json && browserify -p tinyify build/injected/index.js -o build/injected/injected-provider.bundle.js && cp -rf build/injected/injected-provider.bundle.js android/app/src/main/assets/.",
    "start": "npx react-native start",
    "start:reset": "npx react-native start --reset-cache",
    "generate:splash-image": "expo-splash-screen --background-color \"#141828\" --image-path ./src/assets/logo/splash-screen-only-k.png --image-resize-mode contain --status-bar-translucent --status-bar-background-color \"#141828\" --status-bar-style dark-content",
    "pod-install": "pod-install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "sh ./scripts/fix-android-splash-screen-flash.sh && sh ./scripts/force-export-ble-manager-from-ledger.sh"
  },
